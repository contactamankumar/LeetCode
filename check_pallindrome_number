/**
 * @param {number} x
 * @return {boolean}
 */
var isPalindrome = function(x) {
    var flag = false; 
    if(x<0) {
       return false 
    } else {
        var s = (x.toString()).split("").reverse().join("");
        if (x === parseInt(s)){
            return true
        } else {
            return false
        }
    }
    
};
